# Staging Environment Configuration

# Environment name
ENV=staging

# Firebase configuration
FIREBASE_PROJECT_ID=cannasol-executive-dashboard
FIREBASE_HOSTING_URL=https://cannasol-executive-dashboard-staging.web.app

# API endpoints
API_URL=https://us-central1-cannasol-executive-dashboard.cloudfunctions.net/api
CHATBOT_API_URL=https://us-central1-cannasol-executive-dashboard.cloudfunctions.net/chatbot

# Feature toggles
ENABLE_ANALYTICS_LOGGING=true
ENABLE_DEBUG_MODE=false
ENABLE_MOCK_DATA=false

# Performance settings
CACHE_DURATION_MINUTES=15